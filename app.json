    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.maaaashin324.wash-ur-hands",
      "buildNumber": "1.0.4",
      "supportsTablet": false,
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.maaaashin324.wash_ur_hands",
      "versionCode": 1
    }
  }
