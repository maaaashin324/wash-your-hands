{
  "expo": {
    "name": "Washur hands",
    "description": "This app tells you to wash your hands to prevent sickness.",
    "slug": "wash-ur-hands",
    "backgroundColor": "#4297BF",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
